<div class = "contact_page">
  <h1>
     <%= " There are #{Contact.count} contacts"%>
  </h1>

    <ul>
      <% @contacts.each do |contact| %>
        <li>
            <a href="/contacts/<%=contact[:id]%>"> <%= "#{contact.first_name} #{contact.last_name}"%> <%="(#{contact.email})"%> </a>

          <p> <a href="#">Edit</a> | <a href="#">Delete</a> </p>
        </li>
      <% end %>
    </ul>

</div>
